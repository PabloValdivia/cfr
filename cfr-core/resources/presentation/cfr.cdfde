0027 + this.htmlObject).find(\u0027thead\u0027);\n    $.each( $header.find(\u0022th.column0\u0022), function(i,d){ $(d).text(\u0022User\u0022); });\n    $.each( $header.find(\u0022th.column1\u0022), function(i,d){ $(d).text(\u0022File\u0022); });\n    $.each( $header.find(\u0022th.column2\u0022), function(i,d){ $(d).text(\u0022Date\u0022); });\n}\n\n\n "},{"name":"postFetch","type":"JavaScript","value":"function filterData(data) {\n    \n    $(\u0027button#copyBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    $(\u0027button#removeBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    $(\u0027button#downloadBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n\n    \n    if (data.resultset.length === 0)\n    {\n        $(\u0027table#uploadsTableObj\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    }\n    \n    if ($.inArray(\u0022Admin\u0022, this.dashboard.context.roles)\u003E-1) {\n        return data;\n    }\n\n    var array = [];\n    var j = 0;\n    for ( i = 0; i \u003C data.resultset.length; i++ ){\n        \n        if ( this.dashboard.context.user == data.resultset[i][1] ){\n            array[j] = data.resultset[i];\n            j++;\n        }\n    }\n    \n    \n    \n    \n    data.queryInfo.totalRows = j;\n    data.resultset = array;\n    return data;\n} "},{"name":"preChange","type":"JavaScript","value":""},{"name":"postChange","type":"JavaScript","value":""},{"name":"extraOptions","type":"OptionArray","value":"[]"},{"name":"tooltip","type":"Html","value":""},{"name":"clickAction","type":"JavaScript","value":"function click(element)\n{\n    var $elem;\n    \n    var selectedFileParam = \u0022\u0022;\n    \n    if (element.target.attr(\u0022role\u0022) === \u0022row\u0022) {\n        $elem = element.target;\n    } else {\n        $elem = element.target.parent();\n    }\n    \n     // selected file css class\n    var selectedCssClass = \u0027selected\u0027;\n    var selectedFileParamName = \u0027selectedFileParam\u0027\n\n    // remove selected class from any other row\n    $.each( $(\u0027body tr\u0027), function(i, e) {\n                    if (!$(e).is($elem)) $(e).removeClass(selectedCssClass);\n                });\n    // toggle state for\n    $elem.toggleClass(selectedCssClass);\n    \n    if ($elem.hasClass(selectedCssClass)) {\n        selectedFileParam = $elem.find(\u0027.column0\u0027).text();\n        this.dashboard.fireChange(selectedFileParamName, selectedFileParam);\n    } else {\n        this.dashboard.fireChange(selectedFileParamName, selectedFileParam);\n    }\n    \n    \n   if (selectedFileParam)\n   {\n        $(\u0027button#copyBtn\u0027).removeAttr(\u0027disabled\u0027, \u0027disabled\u0027);\n        $(\u0027button#removeBtn\u0027).removeAttr(\u0027disabled\u0027, \u0027disabled\u0027);\n        $(\u0027button#downloadBtn\u0027).removeAttr(\u0027disabled\u0027, \u0027disabled\u0027);\n   }\n   else\n   {\n        $(\u0027button#copyBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n        $(\u0027button#removeBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n        $(\u0027button#downloadBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n   }\n} "}],"type":"ComponentsTable","typeDesc":"Table Component"},{"id":"SCRIPT","name":"Scripts","parent":"UnIqEiD","properties":[{"name":"Group","type":"Label","value":"Scripts"}],"type":"Label","typeDesc":"\u003Ci\u003EGroup\u003C/i\u003E"},{"id":"e9c225bc-1b25-b017-95d7-efa87b7b458b","parent":"SCRIPT","properties":[{"name":"name","type":"Id","value":"Dashboards.init"},{"name":"javaScript","type":"JavaScript","value":"dashboard.postInit = function () {\n    $(\u0022.hidePopup\u0022).removeClass(\u0022hidePopup\u0022);\n    $(\u0022.hidePopup\u0022).removeClass(\u0022hidePopup\u0022);\n    $(\u0027input\u0027).last().addClass(\u0027hide\u0027);\n    \n    $(\u0027button#copyBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    $(\u0027button#removeBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    $(\u0027button#downloadBtn\u0027).attr(\u0027disabled\u0027, \u0027disabled\u0027);\n    \n    $( \u0022#dialogCopyLink\u0022 ).dialog({ \n        autoOpen: false, \n        modal: true,\n        title: \u0022Copy Link\u0022,\n        width: 600,\n        dialogClass: \u0022dlg-no-close\u0022,\n        postition: \u0022center\u0022,\n        resizable: false,\n        draggable: false\n        });\n\n    // File action hooks\n    $(\u0027button#copyBtn\u0027).click(function(e) {\n        \n        var selectedFileParam = dashboard.getParam(\u0022selectedFileParam\u0022);\n        \n        e.preventDefault();\n        \n        if (selectedFileParam) {\n            var msg = \u0027Please press Ctrl+C (Windows/Linux) or Cmd+c (Mac) to copy the link below to clipboard\u0027;\n            \n            var url = window.location.href;\n            url = url.substring(0, url.lastIndexOf(\u0027/\u0027));\n            url = url + \u0027/getFile?fileName=\u0027 + selectedFileParam;\n            \n            $( \u0022#dialogCopyLink\u0022 ).dialog( \u0022option\u0022, \u0022buttons\u0022, \n            [ \n                { text: \u0022Close\u0022, \n                    click: function() { \n                        $( this ).dialog( \u0022close\u0022 ); \n                    } \n                }\n            ]);\n\n            $( \u0022#dialogCopyLink\u0022 ).dialog( \u0022open\u0022); \n            \n            $(\u0022textArea#popup_prompt\u0022).val(url);\n            $(\u0022textArea#popup_prompt\u0022).select();\n        } else {\n            //jAlert(\u0027No file selected!\u0027, \u0027Copy Link\u0027);\n            alert(\u0027No file selected!\u0027);\n        }\n    });\n    \n    $( \u0022#dialogRemove\u0022 ).dialog({ \n        autoOpen: false, \n        modal: true,\n        title: \u0022Remove\u0022,\n        width: 600,\n        dialogClass: \u0022dlg-no-close\u0022,\n        postition: \u0022center\u0022,\n        resizable: false,\n        draggable: false\n        });\n    \n    $(\u0027button#removeBtn\u0027).click(function(e) {\n        \n        var selectedFileParam = dashboard.getParam(\u0022selectedFileParam\u0022);\n        \n        e.preventDefault();\n        \n        if (selectedFileParam) {\n            var url = window.location.href;\n            url = url.substring(0, url.lastIndexOf(\u0027/\u0027));\n            url = url + \u0027/remove?fileName=\u0027 + selectedFileParam;\n            \n            $( \u0022#dialogRemove\u0022 ).dialog( \u0022option\u0022, \u0022buttons\u0022, \n            [ \n                { text: \u0022Close\u0022, \n                    click: function() { \n                        $( this ).dialog( \u0022close\u0022 ); \n                    } \n                },\n                { text: \u0022Remove\u0022,\n                    click: function()\n                    {\n                        $.ajax({url: url, type: \u0022GET\u0022});\n                        render_uploadsTable.update();\n                        $( this ).dialog( \u0022close\u0022 );    \n                    }\n                }\n            ] );\n            $( \u0022p#file_name_remove\u0022 ).html(selectedFileParam);\n            $( \u0022#dialogRemove\u0022 ).dialog( \u0022open\u0022); \n        } else {\n            //jAlert(\u0027No file selected!\u0027, \u0027Remove\u0027);\n            alert(\u0027No file selected!\u0027);\n        }\n    });\n    \n    $( \u0022#dialogDownload\u0022 ).dialog({ \n        autoOpen: false, \n        modal: true,\n        title: \u0022File Download\u0022,\n        width: 600,\n        dialogClass: \u0022dlg-no-close\u0022,\n        postition: \u0022center\u0022,\n        resizable: false,\n        draggable: false\n    });\n        \n    $(\u0027button#downloadBtn\u0027).click(function(e) {\n        \n        var selectedFileParam = dashboard.getParam(\u0022selectedFileParam\u0022);\n        \n        e.preventDefault();\n        \n        if (selectedFileParam) {\n            var msg = \u0027\u003Cp\u003EYou are downloading the file:\u0027 +\n                \u0027\u003Cp class=\u0022text-big text-left\u0022\u003E\u0027 + selectedFileParam + \u0027\u003C/p\u003E\u0027 +\n                \u0027\u003Cp class=\u0022text-center text-bigger\u0022\u003EWould you like to open the file or save it to your computer?\u003C/p\u003E\u0027;\n            \n            $( \u0022#dialogDownload\u0022 ).dialog( \u0022option\u0022, \u0022buttons\u0022, \n            [ \n                { text: \u0022Close\u0022, \n                    click: function() { \n                        $( this ).dialog( \u0022close\u0022 ); \n                    } \n                },\n                { text: \u0022Save\u0022,\n                    click: function()\n                    {\n                        $( this ).dialog( \u0022close\u0022 );\n                        window.location.assign(\u0027/pentaho/plugin/cfr/api/getFile?fileName=\u0027 + selectedFileParam);   \n                    }\n                },\n                { text: \u0022Open\u0022,\n                    click: function()\n                    {\n                        $( this ).dialog( \u0022close\u0022 );\n                        window.open(\u0027/pentaho/plugin/cfr/api/viewFile?fileName=\u0027 + selectedFileParam,\u0027_blank\u0027);   \n                    }\n                }\n            ] );\n            $( \u0022p#file_name_download\u0022 ).html(selectedFileParam);\n            $( \u0022#dialogDownload\u0022 ).dialog( \u0022open\u0022); \n        } else {\n            //jAlert(\u0027No file selected!\u0027, \u0027File Download\u0027);\n            alert(\u0027No file selected!\u0027);\n        }\n    });\n    \n};\n\n "}],"type":"ComponentsFunction","typeDesc":"JavaScript function"},{"id":"CFRCOMPONENTS","name":"CFR","parent":"UnIqEiD","properties":[{"name":"Group","type":"Label","value":"CFR"}],"type":"Label","typeDesc":"\u003Ci\u003EGr